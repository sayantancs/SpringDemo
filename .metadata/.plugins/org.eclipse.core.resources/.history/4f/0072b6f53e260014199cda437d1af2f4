	private List <Point> points ;
	public List<Point> getPoints() {
		return points;
	}
	public void setPoints(List<Point> points) {
		this.points = points;
	}
	public void draw(){
		for (Point p : points){
			System.out.println("["+p.getX()+","+p.getY()+"]");
		}
		
		/*System.out.println("point a=[" + getPointA().getX() +"," +getPointA().getY()  + "]");
